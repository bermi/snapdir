{
  "name": "bermi/snapdir",
  "build": {
    "context": "./../",
    "dockerfile": "./Dockerfile.ubuntu",
    "cacheFrom": ["bermi/snapdir:latest"]
  },
  "settings": {
    "terminal.integrated.defaultProfile.linux": "bash"
  },
  "extensions": [
	"timonwong.shellcheck",
	"github.copilot",
	"rogalmic.bash-debug",
	"redhat.vscode-yaml"
],
  "hostRequirements": {
    "cpus": 1,
    "memory": "1gb",
    "storage": "2gb"
  }
}
